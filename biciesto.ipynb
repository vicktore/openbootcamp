   "source": [
    "\n",
    "def bisiesto(anno: int) -> bool:\n",
    "    return anno % 4 == 0 and (anno % 100 != 0 or anno % 400 == 0)\n",
    "\n",
    "inicio = 2000\n",
    "fin = 3000\n",
    "\n",
    "\n",
    "\n",
    "for anno in range(inicio, fin+1):\n",
    "    if bisiesto(anno):\n",
    "        print(anno, end=\"; \")"
   ]
